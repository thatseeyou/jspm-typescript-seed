{
  "name": "jspm-typescript-seed",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "build": "tsc -p src",
    "build:trace": "tsc -p src --traceResolution",
    "build:debug": "tsc -p src --listFiles --listEmittedFiles",
    "bundle": "cd src && jspm bundle main --inject",
    "unbundle": "cd src && jspm unbundle",
    "bundle-sfx": "cd src && jspm bundle-sfx main build-all.js",
    "serve": "lite-server"
  },
  "author": "",
  "license": "ISC",
  "keywords": [],
  "description": "",
  "devDependencies": {
    "jspm": "^0.16.52",
    "lite-server": "^2.2.2",
    "typescript": "^2.2.1"
  },
  "jspm": {
    "directories": {
      "baseURL": "src",
      "packages": "jspm_packages"
    },
    "configFile": "src/systemjs.config.js",
    "dependencies": {
      "@angular/common": "npm:@angular/common@^4.1.2",
      "@angular/core": "npm:@angular/core@^4.1.2",
      "@angular/forms": "npm:@angular/forms@^4.1.2",
      "@angular/http": "npm:@angular/http@^4.1.2",
      "@angular/platform-browser": "npm:@angular/platform-browser@^4.1.2",
      "@angular/platform-browser-dynamic": "npm:@angular/platform-browser-dynamic@^4.1.2",
      "@angular/router": "npm:@angular/router@^4.1.2",
      "core-js": "npm:core-js@^2.4.1",
      "css": "github:systemjs/plugin-css@^0.1.33",
      "rxjs": "npm:rxjs@^5.4.0",
      "zone.js": "npm:zone.js@^0.8.10"
    },
    "devDependencies": {}
  },
  "dependencies": {
    "@types/node": "^7.0.5"
  }
}
