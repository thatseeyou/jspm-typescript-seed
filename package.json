{
  "name": "jspm-typescript-seed",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "build": "tsc -p src",
    "build:trace": "tsc -p src --traceResolution",
    "build:debug": "tsc -p src --listFiles --listEmittedFiles",
    "bundle": "cd src && jspm bundle app/main --inject",
    "bundle-sfx": "cd src && jspm bundle-sfx app/main build-all.js",
    "serve": "lite-server"
  },
  "author": "",
  "license": "ISC",
  "keywords": [],
  "description": "",
  "devDependencies": {
    "jspm": "^0.16.52",
    "lite-server": "^2.2.2",
    "typescript": "^2.2.1"
  },
  "jspm": {
    "directories": {
      "baseURL": "src",
      "packages": "jspm_packages"
    },
    "configFile": "src/systemjs.config.js",
    "dependencies": {
      "code-prettify": "npm:code-prettify@^0.1.0",
      "css": "github:systemjs/plugin-css@^0.1.32",
      "jquery": "npm:jquery@^3.1.1",
      "jqueryui": "npm:jqueryui@^1.11.1"
    },
    "devDependencies": {}
  },
  "dependencies": {
    "@types/jquery": "^2.0.40",
    "@types/jqueryui": "^1.11.32"
  }
}
